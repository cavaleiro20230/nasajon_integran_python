# API settings
API_KEY=your_api_key_here
PROJECT_NAME=Nasajon Integration API

# CORS settings
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Database settings
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/nasajon_integration

# Nasajon API settings
NASAJON_API_BASE_URL=https://api.nasajon.com/v1
NASAJON_API_KEY=your_nasajon_api_key
NASAJON_CLIENT_ID=your_nasajon_client_id
NASAJON_API_TIMEOUT=30

# Celery settings
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Logging settings
LOG_LEVEL=INFO

